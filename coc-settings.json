{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "explorer.width": 36,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {
       "<cr>": ["expandable?", "expand", "open"],
        "v": "open:vsplit"
   },
   "git.addGBlameToVirtualText": true
}
